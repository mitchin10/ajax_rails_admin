<h1>Stores</h1>
<div class="actions">
  <%= link_to 'New Store', new_store_path, remote: true, class: 'btn btn-primary btn-sm' %>
</div>

<p id="notice"><%= notice %></p>

<table class="table" id="stores">
  <thead>
    <tr>
      <th>Title</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @stores %>
  </tbody>
</table>

<%= render 'dialog' %>
